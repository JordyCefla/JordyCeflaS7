<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="JordyCeflaS6.Vistas.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana de Inicio de Sesión"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el Usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese la Contraseña" IsPassword="True"/>
            <Button x:Name="btnRegistro" Text="Registro" Clicked="btnRegistro_Clicked"/>
            <Button x:Name="btnInicio" Text="Inicio" Clicked="btnInicio_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>